<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ZI_CRYPTER.ViewModel"
                    xmlns:view="clr-namespace:ZI_CRYPTER.View">

    <DataTemplate DataType="{x:Type vm:KodirajVM}">
        <view:TabKodiraj />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DekodirajVM}">
        <view:TabDekodiraj/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PosaljiVM}">
        <view:TabPosalji/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <view:TabSettings/>
    </DataTemplate>

</ResourceDictionary>